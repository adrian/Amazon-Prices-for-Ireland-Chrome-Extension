{
  "name": "Amazon Prices for Ireland",
  "version": "1.0",
  "description": "Decorate product pages on amazon.co.uk with the equivilent Irish price. This price will be in euro and include the Irish VAT rate.",
  "icons": {
    "128": "images/logo-128.png",
    "48": "images/logo-48.png",
    "16": "images/logo-16.png"
  },
  "permissions": [
    "http://download.finance.yahoo.com/*"
  ],
  "background_page" : "background.html",
  "content_scripts": [
    {
      "matches": ["http://www.amazon.co.uk/*",
                  "https://www.amazon.co.uk/*",
                  "http://amazon.co.uk/*",
                  "https://amazon.co.uk/*"],
      "js": ["contentscript.js"]
    }
  ]
}